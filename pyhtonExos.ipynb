#2.10 suite fibonnacci
def fibo(num):
  nombre_1 = 0
  nombre_2 = 1
  total = nombre_1 + nombre_2
  for i in range(num+1):
    if i == 0 or i == 1:
      
      nombre_1 = 0
      nombre_2 = 1
    else:
    
      nombre_1 = nombre_2
      nombre_2  = total
      total = nombre_1 + nombre_2
        
  return total

print("complexité O(n)")
fibo(5)


